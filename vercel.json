{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "vercel-python@3.9.0"
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "api/index.py" }
  ],
  "env": {
    "DATABASE_URL": "postgresql+psycopg2://USER:PASSWORD@HOST:PORT/DBNAME",
    "JWT_SECRET": "CHANGE_ME",
    "JWT_ALGORITHM": "HS256"
  }
}
  